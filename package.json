{
  "name": "baby-announcement",
  "version": "0.1.0",
  "homepage": "https://loading.mckirgan.com",
  "private": true,
  "dependencies": {
    "moment": "^2.25.3",
    "nes-react": "^1.0.2",
    "radium": "^0.26.0",
    "react": "^16.13.1",
    "react-confetti": "^5.0.1",
    "react-cookie": "^4.0.3",
    "react-delay-link": "^1.1.4",
    "react-dinosaur-game": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "0.9.x",
    "react-transition-group": "^1.2.1",
    "react-typist": "^2.0.5",
    "url-loader": "^4.1.0"
  },
  "devDependencies": {
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start"
  }
}
